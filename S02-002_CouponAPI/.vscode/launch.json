  "version": "0.2.0",
  "configurations": [
    {
      "name": "SERVICES COUPON API",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "buildCouponAPI",
      "program": "${workspaceFolder}/Mango.Services.CouponAPI/bin/Debug/net8.0/Mango.Services.CouponAPI.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Mango.Services.CouponAPI",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
        "uriFormat": "%s/swagger/index.html"

      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}